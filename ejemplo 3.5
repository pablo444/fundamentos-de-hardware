Algoritmo CalcularSueldoDelMes
	escribir "ingrese dueldo base" ;
	leer SueldoBase ;
	escribir " cantidad de hijos" ;
	leer CantidadDeHijos;
	escribir " numero de horas trabajadas" ;
	leer NumeroDeHorasTrabajadas
	n1 <- NumeroDeHorasTrabajadastrabajadas -176 ;
	n2 <- CantidadDeHijos *3000 ;
	n3 <- n1*5000 ;
	SueldoDelMes <- SueldoBase+n3+n2
	Escribir " sueldo final en el mes " , SueldoDelMes ;
