Algoritmo cajero_automatico
	Escribir " ingrese dinero ";
	leer dinero;
	//holi
	//esta terea no es tan complicada solo es una flojera/paja mental escribir tantos codigos iguales, asi que las abrebie un poco
	//publico esto con la intencion e ayudarles a organisar sus algoridmos ( y por que me lopidieron ) :)
	b20 <-  trunc(d / 20000);
	Escribir " billetes de $20000 = ",b20;
	d2  <-  d -(b20*20000);
	b10 <- trunc(d2 / 10000);
	escribir  " billetes de $10000 = " ,b10;
	d3 <- d2 - (b10*10000)
	b5<- trunc(d3/ 5000)
	escribir " billetes de $5000 = ",b5;
	d4 <- d3 - (b5*5000);
	b2 <- trunc(d4/2000);
	escribir " billete de $2000 = " ,b2;
	d5 <- d4 - (b2*2000);
	b1 <- trunc(d5/1000);
	escribir " billetes de $1000 = " ,b1;
	d6 <- d5 - (b1*1000);
	m500 <- trunc(d6/500);
	escribir " monedas de $500 = " ,m500
	d7 <- d6 - (m500*500) ;
	m100 <- trunc(d7/100) ;
	escribir " monedas de $100 = ",m100;
	d8 <- d7 - (m100*100) ;
	m50 <- trunc(d8/50);
	escribir " monedas de $50 = ",m50;
	d9 <- d8 - (m50*50) ;
	m10 <- trunc(d9/10) ;
	escribir " monedas de $10 = ", m10;
	d10 <- d9 - (m10*10);
	m5 <- trunc(d10/5) ;
	Escribir "monedas de $5 = ", m5 ;
	d11 <- d10 -(m5*5) ;
	m1 <- trunc(d11/1) ;
	escribir "monedas de $1 = ",m1;
FinAlgoritmo
